<html>
<head>
  <meta charset='utf-8'>
  <title>app_feedback</title>
</head>
<body>
  <h1>Peak Registration Times</h1>

  <table>
      <th>Hour of Day</th><th>Number Registered</th>
        <% fb_hours_array.each do |row| %>
        <tr>
          <td><%= "#{row[0]}" %></td>
          <td><%= "#{row[1]}" %></td>
        </tr>
        <% end %>
  </table>

  <table>
      <th>Day of Week</th><th>Number Registered</th>
        <% fb_day_array.each do |row| %>
        <tr>
          <td><%= "#{row[0]}" %></td>
          <td><%= "#{row[1]}" %></td>
        </tr>
        <% end %>
  </table>
</body>
</html>